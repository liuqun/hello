# @configure_input@
#


# Compiler options
CC := @CC@
CFLAGS := @CFLAGS@

# Useful commands
MKDIR := mkdir -p
RM := rm -rf

# Configure project files
top_level_source_dir := @srcdir@
VPATH := $(top_level_source_dir)
execultable_files :=
source_files  :=
object_files  :=
-include $(top_level_source_dir)/*.mk

#------------------------------
# All building dependences.
.PHONY: all
all: $(execultable_files)
#------------------------------
# Default building rules.
%: %.o
	$(LINK.o) $^ -o $@
%.o: %.c
	$(COMPILE.c) $< -o $@
#------------------------------
# make clean
.PHONY: clean
clean:
	$(RM) $(execultable_files:=@EXEEXT@) $(object_files)
#------------------------------
# make distclean
.PHONY: distclean
distclean: clean
	$(RM) autom4te.cache configure config.log config.status Makefile

